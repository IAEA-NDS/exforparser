  "experimental_conditions": {
    "ANALYSIS": {
      "XX": {
        "0": {
          "freetext": [
            ".RESONANCE-AVERAGE FORMALISM OF LANE AND LYNN WAS USED",
            " TO DETERMINE NEUTRON (P-, D-WAVE) AND PHOTON STRENGTH",
            " FUNCTIONS BY LEAST-SQUARES FIT TO CAPTURE X-SECTIONS.",
            " S-WAVE STRENGTH FUNCTION WAS TAKEN FROM SETH S TABLE."
          ]
        }
      }
    },
    "CORRECTION": {
      "XX": {
        "0": {
          "freetext": [
            ".SELF-SHIELDING AND MULTIPLE-SCATTERING CORRECTIONS",
            " WERE APPLIED FOLLOWING MACKLIN. THEY NEVER EXCEEDED",
            " 8 PERCENT. OTHER CORRECTIONS WERE MADE FOR GAMMA RAY",
            " ESCAPE FROM TANK (ABOUT 3 PERCENT), PULSE HEIGHT",
            " SPECTRUM FRACTION BELOW ELECTRONIC THRESHOLD, AMBIENT",
            " BACKGROUND AND ACCELERATOR-PRODUCED BACKGROUND."
          ]
        }
      }
    },
    "DETECTOR": {
      "XX": {
        "0": {
          "x4code": "(STANK)",
          "freetext": [
            "    800 L LIQUID SCINTILLATOR TANK. BOTH FLIGHT",
            " TIME AND PULSE HEIGHT ARE RECORDED WITH",
            " THRESHOLDS OF ABOUT 3 AND 1 MEV, RESP."
          ]
        }
      }
    },
    "ERR-ANALYS": {
      "XX": {
        "0": {
          "freetext": [
            ".ERRORS ARE ESTIMATED AS FOLLOWS",
            " - ERROR IN REFERENCE CROSS SECTION        3-8 PERCENT",
            " - UNCERT. IN REGISTERED SPECTRUM FRACTION  5  PERCENT",
            " - UNCERT. IN CASCADE ESCAPE PROBABILITY    3  PERCENT",
            " - UNCERT. SELF-SHIELD. AND MULT. SCATT.    1  PERCENT",
            " - STATISTICAL ERROR                      1.5-8PERCENT",
            ".THE RESULTING CROSS SECTION UNCERTAINTY IS",
            " - 13 PERCENT AT 10 KEV,",
            " - 11 PERCENT AT 20 KEV,",
            " -  8 PERCENT AT 30 KEV,",
            " - 10 PERCENT AT 50 KEV AND ABOVE."
          ]
        }
      }
    },
    "FACILITY": {
      "XX": {
        "0": {
          "x4code": [
            "VDG  "
          ],
          "freetext": [
            "    3 MV VAN DE GRAAFF ACCELERATOR, PULSED"
          ]
        }
      }
    },
    "INC-SOURCE": {
      "XX": {
