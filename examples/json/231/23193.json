  "experimental_conditions": {
    "ANALYSIS": {
      "XX": {
        "0": {
          "freetext": [
            "Pulse-height weighting technique (PHWT).",
            "Energy dependence (shape) of the cross-section was",
            " deduced by dividing the capture yield by the neutron",
            " spectrum."
          ]
        }
      }
    },
    "CORRECTION": {
      "XX": {
        "0": {
          "freetext": [
            "Corrected for",
            " - dead-time for 237Np (3.8% at the 1st resonance)",
            " - frame-overlap background",
            "   (40% at 25.3 meV, ~4% at the first resonance,",
            "    including the time-constant component)",
            " - background due to the blank",
            "   (~2.5% at 25.3 meV, ~0.5% at the first resonance)",
            " - background due to the Al materials",
            "   (~7% at 25.3 meV, ~0.2% at the first resonance)",
            " - neutrons scattered by 237Np",
            "   (<0.2% at both 25.3 meV and the first resonance)",
            " - self-shielding and multiple scattering, estimated",
            "   using a Monte-Carlo simulation code PHITS using",
            "   cross sections of JENDL-4.0.",
            "Not corrected for the impurities."
          ]
        }
      }
    },
